ifndef config
  config=debug
endif
export config

PROJECTS := tut_hex

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

tut_hex: 
	@echo "==== Building tut_hex ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f tut_hex.make

clean:
	@${MAKE} --no-print-directory -C . -f tut_hex.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   profile"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   tut_hex"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
